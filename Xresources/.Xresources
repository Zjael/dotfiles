! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: argb:ff292a44, #002b36, #000000
rofi.color-normal: argb:00B2C7D2, argb:ff678bdc, argb:00B2C7D2, argb:00ffffff, argb:ff2de0a7
rofi.color-active: #2de0a7, #ffffff, #2de0a7, #2de0a7, #2de0a7
rofi.color-urgent: #fdf6e3, #dc322f, #eee8d5, #dc322f, #fdf6e3

rofi.fake-transparency: true
rofi.font: lucy tewii 9 
rofi.columns: 2
rofi.separator-style: none
rofi.hide-scrollbar: true
rofi.location: 0
rofi.padding: 300
rofi.fullscreen: true
rofi.width: 1920
rofi.lines: 19

